    MotorEvArgs galng(GALNG, 10, STEPSZ);
    QACTIVE_POST(recv, MOTOR_MOVE_ASOLUTE_SIG, galng.raw);
  }

  if (preSetTimer == 40000) {
    preSetTimer = 6720;
  }
  else {
    preSetTimer += PRESETTIMERINCR;
  }
}

