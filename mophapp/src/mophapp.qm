  }
  else if (preSetTimer &gt;= 3000 &amp;&amp; preSetTimer &lt; 28000) {
    static const uint8_t PROGMEM STEPSZ = 8;
    float target = 60 + 50 * sin((preSetTimer - 3000) / 2500.0 * PI);

    MotorEvArgs uplng(UPLNG, target, STEPSZ);
